<UserControl x:Class="BingoServer.Game.WinScreen"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
              d:DesignHeight="768" d:DesignWidth="1024">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="290*"/>
            <ColumnDefinition Width="449*"/>
            <ColumnDefinition Width="285*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="163*"/>
            <RowDefinition Height="337*"/>
            <RowDefinition Height="105*"/>
            <RowDefinition Height="163*"/>
        </Grid.RowDefinitions>
        
        <Image Name="imgPlayer" Source="/BingoServer;component/Resources/unknown_player.png" Grid.Column="1" Grid.Row="1" Margin="0,5,0,0"/>
        <Image Source="/BingoServer;component/Resources/slingers.png" Grid.ColumnSpan="3"/>
        <Image Source="/BingoServer;component/Resources/proficiat.png" Grid.Column="1" Grid.Row="2" Margin="0,10,0,0"/>
    </Grid>   
    
</UserControl>
